{
  "name": "gtfsrt-lille",
  "version": "1.1.0",
  "description": "un simple module pour avoir directement le gtfs-rt des transports lille",
  "keywords": [
    "ville",
    "france",
    "gtfsrt",
    "gtfs-rt"
  ],
  "homepage": "https://github.com/titoinemb/gtfsrt-lille#readme",
  "bugs": {
    "url": "https://github.com/titoinemb/gtfsrt-lille/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/titoinemb/gtfsrt-lille.git"
  },
  "license": "MIT",
  "author": "titoinemb",
  "types": "./gtfsrt-lille.d.ts",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js",
      "types": "./gtfsrt-lille.d.ts"
    }
  },
  "scripts": {
    "link": "npm link",
    "unlink": "npm unlink",
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc --project tsconfig.esm.json",
    "build:cjs": "tsc --project tsconfig.cjs.json"
  },
  "dependencies": {
    "gtfs-realtime-bindings": "^1.1.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}